from matplotlib import pyplot as plt #as can rename the import to whatever you like as long as you then use that name

x = [1, 2, 3, 4, 5]
fx = [3, 5, 9, 8, 7]

plt.plot(x, fx) #Original - pyplot.plot(x, fx)

#Add title etc...

plt.title('text')
plt.scatter(x, fx)
plt.show()


